{"ast":null,"code":"// Libs\nimport { ref } from 'vue';\nimport router from '@/router';\nimport PsLabel from '@/components/core/label/PsLabel.vue';\nimport PsLink from '@/components/core/link/PsLink.vue';\nimport PsRouteLink from '@/components/core/link/PsRouteLink.vue';\nimport PsConfig from \"@/config/PsConfig\";\nimport PsLabelCaption from '@/components/core/label/PsLabelCaption.vue'; // Holders\n\nimport { PsValueProvider } from \"@/store/modules/core/PsValueProvider\";\nimport { useRecentItemProviderState } from \"@/store/modules/item/RecentItemProvider\";\nimport { usePopularItemProviderState } from '@/store/modules/item/PopularItemProvider';\nimport { usePsAppInfoProviderState } from '@/store/modules/appinfo/AppInfoProvider';\nimport AppInfoParameterHolder from '@/object/holder/AppInfoParameterHolder';\nexport default {\n  components: {\n    PsLabel,\n    PsLink,\n    PsRouteLink,\n    PsLabelCaption\n  },\n\n  setup() {\n    // Inject Item Provider\n    const psValueHolder = PsValueProvider.psValueHolder;\n    const locationId = ref(psValueHolder.locationId);\n    const versionNo = ref(PsConfig.versionNo);\n    const appInfoProvider = usePsAppInfoProviderState();\n    const appInfoParameterHolder = new AppInfoParameterHolder();\n    appInfoParameterHolder.userId = psValueHolder.getLoginUserId(); // Inject Item Provider\n\n    const itempopularProvider = usePopularItemProviderState();\n    const itemrecentProvider = useRecentItemProviderState();\n    itempopularProvider.paramHolder.itemLocationId = locationId.value;\n    appInfoProvider.loadDeleteHistory(appInfoParameterHolder);\n\n    function gotToHome() {\n      router.push({\n        name: \"dashboard\"\n      });\n    }\n\n    function viewAllRecentItemList() {\n      const params = itemrecentProvider.paramHolder.getUrlParamsAndQuery();\n      params['name'] = 'item-list';\n      router.push(params);\n    }\n\n    function viewAllPopularItemList() {\n      const params = itempopularProvider.paramHolder.getUrlParamsAndQuery();\n      params['name'] = 'item-list'; // Redirect\n\n      router.push(params);\n    }\n\n    return {\n      gotToHome,\n      viewAllPopularItemList,\n      viewAllRecentItemList,\n      versionNo,\n      PsConfig,\n      appInfoProvider\n    };\n  }\n\n};","map":{"version":3,"sources":["C:\\Users\\Lenovo\\Documents\\SwaggyCars\\carsayara.com\\src\\components\\layouts\\footer\\FooterLinkSection.vue"],"names":[],"mappings":"AAwHA;AACA,SAAS,GAAT,QAAoB,KAApB;AACA,OAAO,MAAP,MAAmB,UAAnB;AACA,OAAO,OAAP,MAAoB,qCAApB;AACC,OAAO,MAAP,MAAmB,mCAAnB;AACD,OAAO,WAAP,MAAwB,wCAAxB;AACA,OAAO,QAAP,MAAqB,mBAArB;AACA,OAAO,cAAP,MAA2B,4CAA3B,C,CACA;;AACA,SAAS,eAAT,QAAgC,sCAAhC;AACA,SAAS,0BAAT,QAA2C,yCAA3C;AACA,SAAS,2BAAT,QAA4C,0CAA5C;AACA,SAAS,yBAAT,QAA0C,yCAA1C;AACA,OAAO,sBAAP,MAAmC,wCAAnC;AACA,eAAe;AACX,EAAA,UAAS,EAAI;AACT,IAAA,OADS;AAER,IAAA,MAFQ;AAGT,IAAA,WAHS;AAIT,IAAA;AAJS,GADF;;AAOX,EAAA,KAAK,GAAG;AACJ;AACA,UAAM,aAAY,GAAI,eAAe,CAAC,aAAtC;AACA,UAAM,UAAS,GAAI,GAAG,CAAC,aAAa,CAAC,UAAf,CAAtB;AACA,UAAM,SAAQ,GAAI,GAAG,CAAC,QAAQ,CAAC,SAAV,CAArB;AACA,UAAM,eAAc,GAAI,yBAAyB,EAAjD;AACA,UAAM,sBAAqB,GAAI,IAAI,sBAAJ,EAA/B;AACA,IAAA,sBAAsB,CAAC,MAAvB,GAAgC,aAAa,CAAC,cAAd,EAAhC,CAPI,CASJ;;AACA,UAAM,mBAAkB,GAAI,2BAA2B,EAAvD;AACA,UAAM,kBAAiB,GAAI,0BAA0B,EAArD;AACA,IAAA,mBAAmB,CAAC,WAApB,CAAgC,cAAhC,GAAiD,UAAU,CAAC,KAA5D;AAEA,IAAA,eAAe,CAAC,iBAAhB,CAAkC,sBAAlC;;AAEA,aAAS,SAAT,GAAqB;AACjB,MAAA,MAAM,CAAC,IAAP,CAAY;AAAC,QAAA,IAAG,EAAI;AAAR,OAAZ;AACJ;;AAEA,aAAS,qBAAT,GAAiC;AAC7B,YAAM,MAAK,GAAI,kBAAkB,CAAC,WAAnB,CAA+B,oBAA/B,EAAf;AACA,MAAA,MAAM,CAAC,MAAD,CAAN,GAAiB,WAAjB;AAEA,MAAA,MAAM,CAAC,IAAP,CAAY,MAAZ;AAGJ;;AAEA,aAAS,sBAAT,GAAkC;AAE9B,YAAM,MAAK,GAAI,mBAAmB,CAAC,WAApB,CAAgC,oBAAhC,EAAf;AACA,MAAA,MAAM,CAAC,MAAD,CAAN,GAAiB,WAAjB,CAH8B,CAK9B;;AACA,MAAA,MAAM,CAAC,IAAP,CAAY,MAAZ;AAGJ;;AAEA,WAAQ;AACJ,MAAA,SADI;AAEJ,MAAA,sBAFI;AAGJ,MAAA,qBAHI;AAIJ,MAAA,SAJI;AAKJ,MAAA,QALI;AAMJ,MAAA;AANI,KAAR;AAQJ;;AAvDW,CAAf","sourcesContent":["<template>\n    <div class=\"72 text-100 bg-footerColor fl