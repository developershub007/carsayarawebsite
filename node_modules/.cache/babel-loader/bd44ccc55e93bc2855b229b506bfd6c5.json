ry();\n            params['name'] = 'item-list';\n         \n            // Redirect\n            router.push(params);\n            \n\n        }\n\n        return  {\n            gotToHome,\n            viewAllPopularItemList,\n            viewAllRecentItemList,\n            versionNo,\n            PsConfig,\n            appInfoProvider\n        }\n    }\n}\n</script>\n        "],"sourceRoot":""},"metadata":{},"sourceType":"module"}